{
  "allowCompoundWords": true,
  "dictionaries": ["ComPWA", "python"],
  "dictionaryDefinitions": [
    {
      "addWords": true,
      "name": "ComPWA",
      "path": "./.cspell/ComPWA.txt"
    },
    {
      "addWords": true,
      "name": "python",
      "path": "./.cspell/python.txt"
    }
  ],
  "enableFiletypes": [
    "git-commit",
    "github-actions-workflow",
    "julia",
    "jupyter"
  ],
  "flagWords": [
    "analyse",
    "colour",
    "comparision",
    "favour",
    "flavour",
    "hte",
    "optimise",
    "paramater",
    "parmater",
    "transision",
    "transisions"
  ],
  "ignorePaths": [
    "**/.cspell.json",
    ".editorconfig",
    ".gitignore",
    ".gitpod.*",
    ".pre-commit-config.yaml",
    ".prettierignore",
    ".vscode/*",
    "pyproject.toml"
  ],
  "ignoreWords": ["mhutchie", "noqa", "noreply", "taplo"],
  "language": "en-US",
  "version": "0.2",
  "words": []
}
